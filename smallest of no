#include<stdio.h>
int main()
{
 int i,n,a[50],smallest;
 printf("enter the no of elements:");
 scanf("%d",&n);
 printf("enter the elements\n");
 for(i=0;i<n;i++)
 scanf("%d",&a[i]);
 smallest=a[0];
 for(i=0;i<n;i++)
 if(a[i]<smallest)
 {
  smallest=a[i];
  }
 printf("the smallest no is:%d",smallest);
 return 0;
 }
